{
  "expo": {
    "name": "NoteWave",
    "slug": "notewave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0 
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.notewave",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to record videos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shivansh.notewave",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "entryPoint": "./App.js",
    "devDependencies": {
      "@react-native-community/cli": "latest"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "otf"
      ]
    },
    "assets": [
      "./assets/fonts/"
    ],
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a42fd80a-4494-448e-935d-709055b27249"
      }
    },
    "fonts": [
      {
        "asset": "./assets/Mont-ExtraLightDEMO.ttf"
      },
      {
        "asset": "./assets/Mont-HeavyDEMO.ttf"
      },
      {
        "asset": "./assets/Montana.ttf"
      }
    ],
    "owner": "shivansh123"
  }
}
