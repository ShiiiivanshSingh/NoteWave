{
  "expo": {
    "name": "lol",
    "slug": "lol", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to record videos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "entryPoint": "./App.js",
    "devDependencies": {
      "@react-native-community/cli": "latest"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "otf"
      ]
    },
    "assets": [
      "./assets/fonts/"
    ],
    "plugins": [
      "expo-font"
    ]
  }
}
